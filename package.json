{
    "name": "natureshare-web",
    "version": "1.0.0",
    "description": "",
    "scripts": {
        "dev": "next dev -H 0.0.0.0",
        "createObservationsGeoJson": "node --experimental-modules scripts/createObservationsGeoJson.js",
        "createPeoplesCollectionsIndex": "node --experimental-modules scripts/createPeoplesCollectionsIndex.js",
        "fixOldDates": "node --experimental-modules scripts/fixOldDates.js",
        "build": "webpack --config lib/leaflet.webpack.config.cjs && next build",
        "export": "next export",
        "prod": "next start",
        "routes": "node -e 'require(\"./next.config.js\").exportPathMap()'",
        "linter": "eslint --fix --ext .js,.mjs,.cjs,.jsx --ignore-path .gitignore .",
        "prettier": "prettier --write '*.js' '{components,pages,scripts,utils}/**/*.{js,jsx,mjs,cjs}'",
        "serve": "serve -l 3000 out",
        "serveFiles": "serve --cors -l 3001 ../natureshare-files/",
        "serveSpecies": "serve --cors -l 3002 ../natureshare-species-wiki/"
    },
    "author": "Reilly Beacom",
    "license": "",
    "dependencies": {
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
